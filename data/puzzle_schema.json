{
    "$schema": "http://json-schema.org/schema",
    "type": "object",
    "definitions": {
        "day": {
            "type": "object",
            "additionalProperties": false,
            "required": [
                "day",
                "name"
            ],
            "properties": {
                "day": {
                    "type": "number"
                },
                "name": {
                    "type": "string"
                },
                "description": {
                    "type": "string"
                },
                "results": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "required": [
                            "p1"
                        ],
                        "properties": {
                            "p1": {
                                "type": "string"
                            },
                            "p2": {
                                "type": "string"
                            }
                        }
                    }
                },
                "times": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "required": [
                            "environment",
                            "inputs"
                        ],
                        "additionalProperties": false,
                        "properties": {
                            "environment": {
                                "type": "string"
                            },
                            "inputs": {
                                "type": "array",
                                "items": {
                                    "type": "object",
                                    "required": [
                                        "p1"
                                    ],
                                    "properties": {
                                        "p1": {
                                            "type": "number"
                                        },
                                        "p2": {
                                            "type": "number"
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
                "tags": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "comments": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "gotchas": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                }
            }
        },
        "year": {
            "type": "object",
            "required": [
                "year",
                "days"
            ],
            "additionalProperties": false,
            "properties": {
                "year": {
                    "type": "number"
                },
                "days": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/day"
                    }
                }
            }
        },
        "environment": {
            "required": [
                "id",
                "cpu",
                "os",
                "language"
            ],
            "additionalProperties": false,
            "properties": {
                "id": {
                    "type": "string"
                },
                "cpu": {
                    "type": "string"
                },
                "os": {
                    "type": "string"
                },
                "language": {
                    "type": "string",
                    "examples": [
                        "py",
                        "cs"
                    ]
                }
            }
        }
    },
    "required": [
        "years"
    ],
    "additionalProperties": false,
    "properties": {
        "$schema": {
            "type": "string"
        },
        "years": {
            "type": "array",
            "items": {
                "$ref": "#/definitions/year"
            }
        },
        "environments": {
            "type": "array",
            "items": {
                "$ref": "#/definitions/environment"
            }
        }
    }
}